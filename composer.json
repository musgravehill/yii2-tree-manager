{
    "name": "musgravehill/yii2-tree-manager",
    "description": "Tree Manager using jquery.nestable plugin for Yii 2",
    "keywords": [
        "widget",
        "nested sets",
        "yii2",
        "nestable",
        "editor"
    ],
    "homepage": "https://github.com/musgravehill/yii2-tree-manager",
    "type": "yii2-widget",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/musgravehill/yii2-tree-manager/issues",
        "source": "https://github.com/musgravehill/yii2-tree-manager"
    },
    "authors": [
        {
            "name": "voskobovich/yii2-tree-manager"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.0",
        "yiisoft/yii2-bootstrap": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "musgravehill\\tree\\manager\\actions\\": "src/actions",
            "musgravehill\\tree\\manager\\behaviors\\": "src/behaviors",
            "musgravehill\\tree\\manager\\forms\\": "src/forms",
            "musgravehill\\tree\\manager\\interfaces\\": "src/interfaces",
            "musgravehill\\tree\\manager\\widgets\\nestable\\": "src/widgets/nestable"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true 
}
